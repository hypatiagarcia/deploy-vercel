{
    "builds": [
        {
            "src": "python_vercel_task/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python 3.12.3"
            }
        }
         
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python_vercel_task/wsgi.py"
        }
    ]
}